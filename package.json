{
  "name": "namespacestyle",
  "version": "0.0.10",
  "description": "A structured, type-safe link between CSS and HTML.",
  "main": "nss.js",
  "types": "nss.d.ts",
  "files": [
    "nss.d.ts",
    "nss.js",
    "nss.min.js",
    "nss.min.js.gz"
  ],
  "scripts": {
    "build": "tsc && npm run minify",
    "test": "jest",
    "lint": "eslint src/ test/",
    "format": "eslint --fix src/ test/",
    "minify": "esbuild --minify --target=es5 nss.js > nss.min.js && gzip -kf9 nss.min.js && echo \"nss.min.js.gz :: $( ls -lh nss.min.js.gz | tr -s ' ' | cut -d' ' -f5 ) :: $( ls -l nss.min.js.gz | tr -s ' ' | cut -d' ' -f5 )B\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/evnp/nss.git"
  },
  "keywords": [
    "css",
    "html",
    "typescript",
    "javascript"
  ],
  "author": "Evan Purcer",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/evnp/nss/issues"
  },
  "homepage": "https://github.com/evnp/nss#readme",
  "devDependencies": {
    "@types/jest": "^27.0.3",
    "@typescript-eslint/eslint-plugin": "^5.8.1",
    "@typescript-eslint/parser": "^5.8.1",
    "esbuild": "0.14.11",
    "eslint": "^8.5.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-jest": "^25.3.2",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^27.4.5",
    "prettier": "^2.5.1",
    "ts-jest": "^27.1.2",
    "typescript": "^4.5.4"
  },
  "dependencies": {}
}
